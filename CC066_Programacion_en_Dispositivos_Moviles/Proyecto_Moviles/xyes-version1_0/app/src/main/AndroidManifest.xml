            android:name=".InicioApp"
            android:label="@string/title_activity_inicio_app"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".VerNotas"
            android:label="@string/title_activity_ver_notas"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".EdicionDatos"
            android:label="@string/title_activity_edicion_datos"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ModificarActivity"
            android:label="@string/title_activity_modificar"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>
